    "data": {
        "dataset_name":"metr-la",
        "path":"./datasets/metr-la.h5",
        "add_time_in_day": true,
        "freq": "h",
        "lookback":12,
        "horizon":12,
        "channel":207,
        "normalize": 3,
        "train_ratio":0.6,
        "valid_ratio":0.2,
        "test_ratio":0.2,
        "multivariate":true,
        "target": "OT",
        "scalar":"StandardScalar"
