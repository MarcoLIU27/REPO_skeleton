{
    "exp":{
        "train":{
            "training": true,
            "saved_model": true,
            "batchsize":64,
            "epochs":100,
            "lr":0.001,
            "lr_adj":1,
            "shuffle": true,
            "drop_last": true,
            "step_size": 100,
            "optimizer": "adam",
            "loss": "mae",
            "patience": 7,
            "verbose": true,
            "delta": 0
        },
        "valid":{
            "batchsize":64,
            "shuffle": true,
            "drop_last": true
        },
        "test":{
            "batchsize":64,
            "shuffle": false,
            "drop_last": true
        },
        "metrics":{
            "MSE": true,
            "MAE": true,
            "RMSE": true,
            "MAPE": true,
            "RSE": true,
            "CORR": true
        },
        "device":"cuda"
    },


    "model": { 
        "model_name":"MTGNN",
        "gcn_true": true,
        "buildA_true": true,
        "gcn_depth": 2,
        "num_nodes": 207,
        "device": "cuda",
        "dropout": 0.3,
        "subgraph_size": 20,
        "node_dim": 40,
        "dilation_exponential": 1,
